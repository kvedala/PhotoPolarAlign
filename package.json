{
  "name": "photopolaralign",
  "productName": "Photo Polar Align",
  "version": "0.0.1",
  "description": "Utility to help align German Equatorial telescope mount by imaging the Celestial Pole region.",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "electron .",
    "dev": "electron . --debug",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=images/PPALogo.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=images/PPA.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/photopolaralign-win32-ia32/photopolaralign.exe' --cert ~/photopolaralign.p12 --prompt ",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/photopolaralign.exe' --cert ~/photopolaralign.p12 --prompt",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvedala/PhotoPolarAlign.git"
  },
  "keywords": [
    "polar",
    "align",
    "equatorial",
    "mount",
    "astrophotography"
  ],
  "author": "Krishna Vedala <krishna.vedala@ieee.org>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/kvedala/PhotoPolarAlign/issues"
  },
  "homepage": "https://github.com/kvedala/PhotoPolarAlign#readme",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^4.1.1",
    "electron-installer-windows": "^1.1.1",
    "electron-settings": "^3.2.0",
    "update-electron-app": "^1.3.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}