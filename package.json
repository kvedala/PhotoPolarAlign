{
  "name": "photopolaralign",
  "productName": "Photo Polar Align",
  "version": "0.0.1",
  "description": "Utility to help align German Equatorial telescope mount by imaging the Celestial Pole region.",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "electron .",
    "exe32": "electron-packager . photopolaralign --platform win32 --arch ia32 --out dist/ --icon ./images/PPA.ico",
    "exe64": "electron-packager . photopolaralign --platform win32 --arch x64 --out dist/ --icon ./images/PPA.ico",
    "set32": "electron-installer-windows --src dist/photopolaralign-win32-ia32/ --dest dist/installers/ia32/",
    "set64": "electron-installer-windows --src dist/photopolaralign-win32-x64/ --dest dist/installers/x64/",
    "build": "npm run clean && npm run exe32 && npm run set32 && npm run exe64 && npm run set64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvedala/PhotoPolarAlign.git"
  },
  "keywords": [
    "polar",
    "align",
    "equatorial",
    "mount",
    "astrophotography"
  ],
  "author": "Krishna Vedala <krishna.vedala@ieee.org>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/kvedala/PhotoPolarAlign/issues"
  },
  "homepage": "https://github.com/kvedala/PhotoPolarAlign#readme",
  "devDependencies": {
    "electron": "^4.1.1",
    "electron-installer-windows": "^1.1.1",
    "electron-settings": "^3.2.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
